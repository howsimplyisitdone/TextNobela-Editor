            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ContactActivity">
        </activity>
        <activity android:name=".CreateContactActivity">
        </activity>
        <activity android:name=".ContactDetailActivity">
        </activity>
        <activity android:name=".MessageActivity">
        </activity>
        <activity android:name=".ChatActivity">
        </activity>
        <activity android:name=".CreateMessageActivity">
        </activity>
        <activity android:name=".SelectContactActivity">
        </activity>
        <activity android:name=".MessageDetailActivity">
        </activity>
        <activity android:name=".SettingActivity">
        </activity>
    </application>

</manifest>